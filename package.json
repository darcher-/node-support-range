{
  "activationEvents": [
    "onLanguage:json",
    "onLanguage:jsonc",
    "workspaceContains:package.json"
  ],
  "bugs": {
    "email": "dylarcher@gmail.com",
    "url": "https://github.com/darcher-/node-support-range/issues"
  },
  "categories": [
    "Linters",
    "Programming Languages",
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "category": "NodeSupportLimits",
        "command": "node-support-limits.analyzeDependencies",
        "title": "Supported NodeJS/NPM versions range"
      },
      {
        "category": "NodeSupportLimits",
        "command": "node-support-limits.analyzeDependencies",
        "title": "Compatible NodeJS/NPM version limits"
      },
      {
        "category": "NodeSupportLimits",
        "command": "node-support-limits.analyzeDependencies",
        "title": "NodeJS/NPM minimum/maximum versions"
      },
      {
        "category": "NodeSupportLimits",
        "command": "node-support-limits.analyzeDependencies",
        "title": "Dependency node/npm version constraints"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "node-support-limits.analyzeDependencies",
          "when": "resourceLangId == jsonl"
        },
        {
          "command": "node-support-limits.analyzeDependencies",
          "when": "resourceLangId == json5"
        },
        {
          "command": "node-support-limits.analyzeDependencies",
          "when": "resourceLangId == jsonc"
        },
        {
          "command": "node-support-limits.analyzeDependencies",
          "when": "resourceLangId == json"
        }
      ],
      "explorer/context": [
        {
          "command": "node-support-limits.analyzeDependencies",
          "group": "navigation@5",
          "when": "resourceFilename == package.json"
        },
        {
          "command": "node-support-limits.analyzeDependencies",
          "group": "navigation@5",
          "when": "resourceFilename == package-lock.json"
        },
        {
          "command": "node-support-limits.analyzeDependencies",
          "group": "navigation@5",
          "when": "resourceFilename == npm-shrinkwrap.json"
        }
      ]
    }
  },
  "dependencies": {
    "semver": "7.7.1"
  },
  "description": "Analyzes project dependencies to determine and suggest NodeJS/NPM version support for the project's `package:engines` entries.",
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/mocha": "10.0.10",
    "@types/node": "22.15.17",
    "@types/semver": "7.7.0",
    "@types/vscode": "1.100.0",
    "@vscode/test-electron": "2.5.2",
    "conventional-changelog-cli": "5.0.0",
    "glob": "11.0.2",
    "mocha": "11.2.2",
    "vscode-test": "1.6.1"
  },
  "displayName": "Node Support Range",
  "engines": {
    "node": ">=18.0.0 <=24.0.1",
    "npm": ">=10.8.2 <=11.3.0",
    "vscode": "^1.100.0"
  },
  "main": "src/extension.js",
  "name": "node-support-range",
  "packageManager": "npm@11.3.0",
  "publisher": "darcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/darcher-/node-support-range.git"
  },
  "scripts": {
    "postinstall": "npm outdated",
    "postpack": "npm find-dupes",
    "preinstall": "npm up --ignore-scripts",
    "prepack": "npm prune --ignore-scripts",
    "prepare": "npm ci --ignore-scripts",
    "update:changelog": "conventional-changelog",
    "test": "node ./src/test/runTest.js",
    "test:basic": "mocha ./src/test/suite/basic.test.js",
    "test:helper": "mocha ./src/test/suite/helpers.test.js",
    "test:extension": "mocha ./src/test/suite/extension.test.js",
    "test:constants": "mocha ./src/test/suite/constants.test.js",
    "test:standalone": "mocha ./src/test/suite/**/*.test.js",
    "test:standalone:all": "node ./src/test/run-standalone-tests.js",
    "test:standalone:helper": "node ./src/test/helper-test.js",
    "test:standalone:helper:module": "node ./src/test/helper-module-test.js",
    "test:standalone:basic": "node ./src/test/basic-import-test.js",
    "test:standalone:constants": "node ./src/test/constants-test.js"
  },
  "type": "module",
  "version": "1.0.0"
}
